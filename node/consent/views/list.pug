extends layout
include mixins

block content
  +nav("Consent Handling", "dropdown_menu", "inverse")
      +nav_item( "list" ) List of consents
      +nav_item( "logout" ) Logout
  .container-fluid
    h1 Consent Listing
    div
      table.table.table-hover
        thead
          tr
            th(wdith="30%") Contract
            th(width="60%") Title
            th(width="5%") Version
            th(width="5%") Status
        tbody
          each val in consents
            tr
              td= val.id
              td= val.title
              td= val.version
              td= val.status

